# Gradient Agent Configuration
# Copy this to .env and fill in your values

# DigitalOcean API Token (for deployment)
DIGITALOCEAN_API_TOKEN=dop_v1_your-token-here

# Gradient Model Access Key (for inference)
GRADIENT_MODEL_ACCESS_KEY=sk-do-your-key-here

# Model to use (default: oss-gpt:20b)
GRADIENT_MODEL=openai-gpt-oss-120b

# Cosmos DB Configuration (inherited from parent .env)
# COSMOS_ACCOUNT_NAME=your-cosmos-account
# COSMOS_PRIMARY_KEY=your-cosmos-key
# COSMOS_DATABASE=referral-network
# COSMOS_GRAPH=hospital-graph
